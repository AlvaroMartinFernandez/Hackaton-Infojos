{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST"],  // Métodos HTTP permitidos para esta ruta
      "maxDuration": 65 // Tiempo máximo de espera en segundos
    }
  ]
}
